{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "react",
    "react-hooks"
  ],
  "rules": {
    "react/prop-types": [
      "error",
      {
        "ignore": [
          "className",
          "type"
        ]
      }
    ],
    "@typescript-eslint/no-empty-object-type": "off"
  }
}